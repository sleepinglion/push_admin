<%= form_for [:admin,push_prepare_picture], :html => { :multipart => true } do |f| %>
<% if push_prepare_picture.errors.any? %>
<div id="error_explanation" class="alert alert-danger">
	<a href="#" class="close" data-dismiss="alert">&times;</a>
	<h2><%= pluralize(product.errors.count, "error") %> prohibited this request from being saved:</h2>
	<ul>
		<% push_prepare_picture.errors.full_messages.each do |msg| %>
		<li>
			<%= msg %>
		</li>
		<% end %>
	</ul>
</div>
<% end %>
<div class="form-group">
	<%= f.label :photo %>
	<%= f.file_field :photo %>
	<%= f.hidden_field :photo_cache %>
</div>
<div class="form-group">
	<%= f.label :enable %>
	<%= f.check_box :enable %>
</div>
<div class="form-group actions">
	<%= f.submit :class=>"btn btn-primary" %>
</div>
<% end %>
